module Legalesign
  module Models
    type template_create_params =
      { group: String, latest_text: String, title: String, user: String }
      & Legalesign::Internal::Type::request_parameters

    class TemplateCreateParams < Legalesign::Internal::Type::BaseModel
      extend Legalesign::Internal::Type::RequestParameters::Converter
      include Legalesign::Internal::Type::RequestParameters

      attr_accessor group: String

      attr_accessor latest_text: String

      attr_accessor title: String

      attr_reader user: String?

      def user=: (String) -> String

      def initialize: (
        group: String,
        latest_text: String,
        title: String,
        ?user: String,
        ?request_options: Legalesign::request_opts
      ) -> void

      def to_hash: -> {
        group: String,
        latest_text: String,
        title: String,
        user: String,
        request_options: Legalesign::RequestOptions
      }
    end
  end
end
